    "@vercel/ncc": "^0.34.0",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.2.5",
